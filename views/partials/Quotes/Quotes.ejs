<footer>
<div class="my-container container align-bottom ">
    <hr>
    <div class="text-center">
    <figure>
        <blockquote class="blockquote">
            <i>"<%=Quotes.TEXT_BODY %>"</i>
            <figcaption class="blockquote-footer">
                <%if(Quotes.CHARACTER != null){%>
                    <%=Quotes.CHARACTER%>,
                <% } %>
                <cite>
                    <%if(Quotes.BOOK_ID != null){%>
                        (<a = class = "single" href = "/books/<%=Quotes.BOOK_ID%>" ><%=  Quotes.TITLE  %></a>)
                    <% } %>
                </cite>
            </figcaption>
        </blockquote>
    </figure>
    </div>
</div>
</footer>
